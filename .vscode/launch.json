{
    "version": "1.0.0",
    "configurations": [
        {
            "type": "java",
            "name": "BackApplication",
            "request": "launch",
            "mainClass": "com.webShop.back.BackApplication",
            "projectName": "lab2"
        },
        {
            "name": "Start App",
            "runtimeVersion": "16.0.0",
            "runtimeArgs": [
                "start"
            ],
            "env": {
                "BROWSER": "none"
            },
            "request": "launch",
            "runtimeExecutable": "npm",
            "type": "node"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Run Chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ],
    "compounds": [
        {
          "name": "Debug App",
          "configurations": ["Start App", "Run Chrome"],
          "stopAll": true
        }
    ]
}